        <hashTree>
          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import java.util.UUID

def messageId = UUID.randomUUID().toString()
def timestamp = System.currentTimeMillis()

def payload = &quot;&quot;&quot;
{
  &quot;ID&quot;: &quot;${messageId}&quot;,
  &quot;Payload&quot;: &quot;${__RandomString(50)}&quot; 
  &quot;Message&quot;: &quot;Sample message for TestCrew Task from Ubaid&quot;
}
&quot;&quot;&quot;

vars.put(&quot;messageContent&quot;, payload)
vars.put(&quot;messageId&quot;, messageId)

new File(&quot;message_ids.txt&quot;) &lt;&lt; messageId + &quot;\n&quot;</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
